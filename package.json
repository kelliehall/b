  "scripts": {
    "webpack-dev": "cross-env NODE_ENV=development webpack",
    "webpack-prod": "cross-env NODE_ENV=prod webpack",
    "build": "webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details --display-cached",
    "buildp": "webpack --config config/webpack.prod.js --progress --profile --colors --display-error-details --display-cached",
    "watch": "npm run build -- --watch",
    "clean": "run-s clean:coverage clean:dist",
    "clean:coverage": "del-cli coverage",
    "clean:dist": "del-cli dist",
    "lint": "run-s lint:js lint:ts",
    "lint:js": "eslint -c .eslintrc.yml *.js app",
    "lint:ts": "tslint src/app/**/*.ts",
    "prebuild": "npm run clean:dist",
    "pretest": "run-s clean:coverage lint",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "start": "npm run server:dev",
    "test": "cross-env NODE_ENV=test karma start"
  },
